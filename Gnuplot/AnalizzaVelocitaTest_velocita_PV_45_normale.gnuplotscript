#Analizza i dati
set fit logfile '../Risultati/Analisi_Dati/ris_45gradi_peso'

f(x)=a+b*x

fit f(x) '< paste ../Risultati/MetaDati/45/arrayTempi_PrimaVolta_peso.txt ../Risultati/MetaDati/45/velocita_PV_peso.txt' using 1:2 via a,b





#Fai il grafico
set xlabel "Tempo (s)"
set ylabel "Velocita (m/s)"
unset key

set terminal png
set output "./immagini/velocita_45gradi_normale.png"

plot '< paste ../Risultati/MetaDati/45/arrayTempi_PrimaVolta_normale.txt ../Risultati/MetaDati/45/velocita_PV_normale.txt' with yerrorbars, \
f(x)


unset output
