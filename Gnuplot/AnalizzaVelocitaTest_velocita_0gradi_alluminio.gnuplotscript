#Analizza i dati
set fit logfile '../Risultati/Analisi_Dati/ris_0gradi_alluminio'

f(x)=a+b*x

fit f(x) '< paste ../Risultati/MetaDati/0/arrayTempi_0gradi_alluminio.txt ../Risultati/MetaDati/0/velocita_0gradi_alluminio.txt' using 1:2 via a,b



#Fai il grafico
set xlabel "Tempo (s)"
set ylabel "Velocita (m/s)"
unset key

set terminal png
set output "./immagini/velocita_0gradi_alluminio.png"

plot '< paste ../Risultati/MetaDati/0/arrayTempi_0gradi_alluminio.txt ../Risultati/MetaDati/0/velocita_0gradi_alluminio.txt' with yerrorbars, \
f(x)

unset output
